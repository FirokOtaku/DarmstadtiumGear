{
  "main": "index.html",
  "logo": "darmstadtium-gear-icon.png",
  "preload": "preload.js",

  "development": {
    "main": "http://localhost:5173/"
  },

  "features": [
    {
      "code": "search-by-default",
      "explain": "使用默认搜索引擎, 搜索指定关键字",
      "cmds": [
        {
          "type": "over",
          "label": "直接搜索",
          "match": "*",
          "minLength": 1,
          "maxLength": 128
        }
      ]
    },
    {
      "code": "search-config",
      "explain": "配置搜索引擎列表",
      "cmds": [
        {
          "type": "over",
          "label": "配置\uD86D\uDFFC齿轮",
          "match": "config",
          "minLength": 6,
          "maxLength": 12
        }
      ]
    }
  ],

  "pluginSetting": {
    "single": true
  }
}
